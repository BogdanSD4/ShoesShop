@{
    Layout = "_EmptyLayout";
}

<div class="login-form">
    <h2>Login</h2>
    <form>
        <label for="username">User name:</label>
        <input type="text" id="username" name="username">
        <br>
        <label for="password">Password:</label>
        <input type="password" id="password" name="password">
        <br>
        <button class="button" onclick="sendLoginData(event)">Enter</></button>
        <br>
        <label class="link">You dont have account?
            <a class="link" href="../Login/SignIn">Sign in</a>
        </label>
    </form>
</div>
